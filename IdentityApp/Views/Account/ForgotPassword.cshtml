@model LoginViewModel

<h2>Forgot My Paassword</h2>
<hr>
<form asp-action="ForgotPassword" method="post">
    
    <div class="mb-3">
        <label for="Email"></label>
        <input name="Email" type="email" class="form-control"/>
    </div>
        
    <button class="btn btn-primary">Send</button>
</form>